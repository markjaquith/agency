{
  "name": "@markjaquith/agency",
  "version": "0.7.0",
  "description": "Manages personal agents files",
  "license": "MIT",
  "author": "Mark Jaquith",
  "repository": {
    "type": "git",
    "url": "https://github.com/markjaquith/agency"
  },
  "module": "index.ts",
  "type": "module",
  "bin": {
    "agency": "cli.ts"
  },
  "scripts": {
    "test": "find src -name '*.test.ts' -print0 | xargs -0 -n 1 -P 4 bun test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "knip --production",
    "pushable": "./scripts/pushable",
    "check-commit-msg": "./scripts/check-commit-msg",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish --provenance",
    "build": "bun build cli.ts --outdir dist --target node --minify"
  },
  "exports": {
    ".": {
      "import": "./index.ts",
      "types": "./index.ts"
    }
  },
  "files": [
    "index.ts",
    "cli.ts",
    "src",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "agents",
    "bun",
    "typescript",
    "personal-agents"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "knip": "^5.70.1",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "effect": "^3.19.6",
    "jsonc-parser": "^3.3.1",
    "ora": "^8.1.1"
  },
  "trustedDependencies": [
    "@triggi/native-exec"
  ]
}
