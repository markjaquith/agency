{
  "name": "@markjaquith/agency",
  "version": "0.0.0-development",
  "description": "Manages personal agents files",
  "keywords": [
    "agents",
    "bun",
    "personal-agents",
    "typescript"
  ],
  "license": "MIT",
  "author": "Mark Jaquith",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markjaquith/agency.git"
  },
  "bin": {
    "agency": "cli.ts"
  },
  "files": [
    "index.ts",
    "cli.ts",
    "src",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "module": "index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "scripts": {
    "test": "find src -name '*.test.ts' ! -name '*.integration.test.ts' -print0 | xargs -0 -n 1 -P 4 bun test",
    "test:integration": "find src -name '*.integration.test.ts' -print0 | xargs -0 -n 1 -P 4 bun test",
    "test:all": "find src -name '*.test.ts' -print0 | xargs -0 -n 1 -P 4 bun test",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "knip": "knip --production",
    "pushable": "./scripts/pushable",
    "check-commit-msg": "./scripts/check-commit-msg",
    "build": "bun build cli.ts --outdir dist --target node --minify"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "effect": "^3.19.15",
    "jsonc-parser": "^3.3.1",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "knip": "^5.82.1",
    "oxfmt": "^0.27.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "@triggi/native-exec"
  ]
}
